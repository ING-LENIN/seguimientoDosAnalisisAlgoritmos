@INPROCEEDINGS{9314935,
  author={Fong, A.C.M. and Usman, Muhammad},
  booktitle={2020 IEEE 2nd International Workshop on System Biology and Biomedical Systems (SBBS)}, 
  title={Machine Thinking for Telehealth – Toward the Next Generation of Healthcare}, 
